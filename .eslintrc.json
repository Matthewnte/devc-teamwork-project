{
    "env": {
        "browser": true,
        "es6": true,
        "node": true
    },
    "extends": [
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "eol-last": "off",
        "linebreak-style": "off",
        "indent": "off",
        "semi": "off",
        "no-undef": "warn",
        "quote-props": "off",
        "quotes": "off",
        "no-unused-vars": "warn",
        "consistent-return": "off",
        "no-return-assign": "off",
        "object-curly-newline": "off"
    }
}